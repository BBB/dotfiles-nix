"*":
  "advanced-open-file":
    createDirectories: true
    createFileInstantly: true
    helmDirSwitch: true
  "autocomplete-modules":
    babelPluginModuleResolver: true
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    defaultProvider: "Fuzzy"
    enableAutoConfirmSingleSuggestion: false
    minimumWordLength: 2
    useLocalityBonus: false
  autosave:
    enabled: true
  "color-picker":
    abbreviateValues: true
    preferredFormat: "HEX"
  core:
    automaticallyUpdate: false
    customFileTypes:
      "source.ini": [
        ".hgrc"
        ".buckconfig"
        ".flowconfig"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "language-c"
      "language-clojure"
      "language-csharp"
      "language-go"
      "language-java"
      "language-mustache"
      "language-objective-c"
      "language-perl"
      "language-php"
      "language-python"
      "language-toml"
      "language-xml"
      "linter"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "node_modules"
    ]
    packagesWithKeymapsDisabled: [
      "tool-bar"
      "zentabs"
      "maximize-panes"
      "color-picker"
      "todo-show"
      "git-blame"
      "git-plus"
    ]
    telemetryConsent: "limited"
    themes: [
      "nucleus-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
    titleBar: "hidden"
  "custom-title":
    template: "atom/<%= projectName %>"
  editor:
    fontFamily: "Source Code Pro"
    fontSize: 16
    invisibles:
      eol: " "
      space: " "
    preferredLineLength: 100
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "f07c48e3-36eb-b22e-de8c-423ac96973dd"
  "file-icons":
    coloured: false
  "find-and-replace":
    projectSearchResultsPaneSplitDirection: "right"
  "flex-tool-bar": {}
  "git-blame":
    columnWidth: 350
    showOnlyLastNames: false
  "git-diff": {}
  "git-plus":
    diffs:
      syntaxHighlighting: false
    general:
      analytics: false
      splitPane: "Right"
  "highlight-selected": {}
  linter:
    displayLinterInfo: false
    displayLinterStatus: false
    showErrorTabFile: false
    showErrorTabProject: false
    statusIconScope: "File"
  "linter-eslint": {}
  "linter-scss-lint":
    disableWhenNoConfigFileInPath: true
  "markdown-preview":
    useGitHubStyle: true
  "nice-index":
    fileNames: [
      "index\\.|styles\\.s?css"
    ]
  nuclide:
    "atom-ide-diagnostics-ui": {}
    "nuclide-code-format": {}
    "nuclide-diagnostics-ui": {}
    "nuclide-flow":
      canUseFlowBin: true
      enablePushDiagnostics: true
      enableStructuredTypeHints: true
      flowAutocompleteResultsFirst: false
      functionSnippetShouldIncludeArguments: false
    use:
      "nuclide-adb-logcat": false
      "nuclide-arcanist": false
      "nuclide-blame": false
      "nuclide-blame-provider-hg": false
      "nuclide-bookshelf": false
      "nuclide-buck": false
      "nuclide-buck-ios": false
      "nuclide-busy-signal": false
      "nuclide-clang": false
      "nuclide-clipboard-path": false
      "nuclide-console": false
      "nuclide-context-view": false
      "nuclide-ctags": false
      "nuclide-debugger": false
      "nuclide-debugger-iwdp": false
      "nuclide-debugger-native": false
      "nuclide-debugger-node": false
      "nuclide-debugger-php": false
      "nuclide-diff-view": false
      "nuclide-distraction-free-mode": false
      "nuclide-file-tree": false
      "nuclide-file-watcher": false
      "nuclide-fuzzy-filename-provider": false
      "nuclide-hack": false
      "nuclide-health": false
      "nuclide-hg-conflict-resolver": false
      "nuclide-hg-repository": false
      "nuclide-hhvm": false
      "nuclide-home": false
      "nuclide-http-request-sender": false
      "nuclide-ios-simulator-logs": false
      "nuclide-language-hack": false
      "nuclide-nux": false
      "nuclide-objc": false
      "nuclide-open-filenames-provider": false
      "nuclide-outline-view": false
      "nuclide-python": false
      "nuclide-quick-open": false
      "nuclide-recent-files-provider": false
      "nuclide-recent-files-service": false
      "nuclide-refactorizer": false
      "nuclide-related-files": false
      "nuclide-remote-atom": false
      "nuclide-remote-projects": false
      "nuclide-settings": false
      "nuclide-source-control-side-bar": false
      "nuclide-swift": false
      "nuclide-task-runner": false
      "nuclide-vcs-log": false
      "nuclide-working-sets": false
  "prettier-atom":
    formatOnSaveOptions:
      cssScopes: [
        "source.css"
      ]
      enabled: true
    prettierEslintOptions: {}
    prettierOptions:
      singleQuote: true
      trailingComma: "all"
    silenceErrors: true
    useEslint: true
  tabs:
    enableVcsColoring: true
  "todo-show":
    sortBy: "Type"
  "tool-bar":
    position: "Left"
  "tree-view":
    alwaysOpenExisting: true
  "vim-mode-plus":
    dontUpdateRegisterOnChangeOrSubstitute: true
    useClipboardAsDefaultRegister: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
  zentabs:
    maximumOpenedTabs: 20
    neverCloseNew: true
