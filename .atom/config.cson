"*":
  "advanced-open-file":
    createDirectories: true
    createFileInstantly: true
    helmDirSwitch: true
  "autocomplete-modules":
    babelPluginModuleResolver: true
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    defaultProvider: "Fuzzy"
    enableAutoConfirmSingleSuggestion: false
    minimumWordLength: 2
    useLocalityBonus: false
  autosave:
    enabled: true
  "color-picker":
    abbreviateValues: true
    preferredFormat: "HEX"
  core:
    automaticallyUpdate: false
    customFileTypes:
      "source.ini": [
        ".hgrc"
        ".buckconfig"
        ".flowconfig"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "language-c"
      "language-clojure"
      "language-csharp"
      "language-java"
      "language-mustache"
      "language-objective-c"
      "language-perl"
      "language-php"
      "language-python"
      "language-xml"
      "linter"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "node_modules"
    ]
    packagesWithKeymapsDisabled: [
      "tool-bar"
      "zentabs"
      "maximize-panes"
      "color-picker"
      "todo-show"
      "git-blame"
      "git-plus"
      "highlight-selected"
    ]
    telemetryConsent: "limited"
    themes: [
      "nucleus-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
    titleBar: "hidden"
  "custom-title":
    template: "atom/<%= projectName %>"
  editor:
    fontFamily: "Hermit, Menlo"
    fontSize: 18
    invisibles:
      eol: " "
      space: " "
    preferredLineLength: 100
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "9ff2ff58-de25-458a-a6d5-b4e2e4a30393"
  "file-icons":
    coloured: false
  "find-and-replace":
    projectSearchResultsPaneSplitDirection: "right"
  "flex-tool-bar": {}
  "git-blame":
    columnWidth: 350
    showOnlyLastNames: false
  "git-diff": {}
  "git-plus":
    diffs:
      syntaxHighlighting: false
    general:
      analytics: false
      splitPane: "Right"
  "go-plus":
    disabledBundledPackages: [
      "go-debug"
      "linter"
    ]
    lint:
      args: [
        "--vendor"
        "--disable-all"
        "--enable=vet"
        "--enable=vetshadow"
        "--enable=ineffassign"
        "--enable=goconst"
        "--tests"
        "--json"
        "."
      ]
  "highlight-selected": {}
  linter:
    displayLinterInfo: false
    displayLinterStatus: false
    showErrorTabFile: false
    showErrorTabProject: false
    statusIconScope: "File"
  "linter-eslint": {}
  "linter-scss-lint":
    disableWhenNoConfigFileInPath: true
  "markdown-preview":
    useGitHubStyle: true
  "nice-index":
    fileNames: [
      "index\\.|styles\\.s?css"
    ]
  nuclide:
    "atom-ide-code-format":
      formatOnSave: true
      formatOnType: false
    "atom-ide-datatip":
      datatipDebounceDelay: 400
    "atom-ide-diagnostics-ui": {}
    "nuclide-code-format": {}
    "nuclide-diagnostics-ui": {}
    "nuclide-flow":
      canUseFlowBin: true
      enablePushDiagnostics: true
      enableStructuredTypeHints: true
      flowAutocompleteResultsFirst: false
      functionSnippetShouldIncludeArguments: false
    "nuclide-health":
      toolbarJewel: "None"
    use:
      "nuclide-adb-logcat": false
      "nuclide-adb-sdb": false
      "nuclide-arc-lint": false
      "nuclide-arcanist": false
      "nuclide-blame": false
      "nuclide-blame-provider-hg": false
      "nuclide-bookshelf": false
      "nuclide-buck": false
      "nuclide-buck-ios": false
      "nuclide-clang": false
      "nuclide-clipboard-path": false
      "nuclide-console": false
      "nuclide-debugger": false
      "nuclide-debugger-native": false
      "nuclide-debugger-php": false
      "nuclide-device-panel": false
      "nuclide-distraction-free-mode": false
      "nuclide-file-tree": false
      "nuclide-hack": false
      "nuclide-health": false
      "nuclide-hg-repository": false
      "nuclide-home": false
      "nuclide-http-request-sender": false
      "nuclide-language-hack": false
      "nuclide-objc": false
      "nuclide-python": false
      "nuclide-quick-open": false
      "nuclide-remote-atom": false
      "nuclide-remote-projects": false
      "nuclide-settings": false
      "nuclide-ssh-tunnel": false
      "nuclide-vcs-log": false
      "nuclide-working-sets": false
  "pg-formatter":
    editorOptions: {}
    pgFormatterOptions:
      functionCase: "uppercase"
      keywordCase: "uppercase"
      placeholder: "(password|timezone|enum)"
      spaces: 2
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
      isDisabledIfNoConfigFile: true
      isDisabledIfNotInPackageJson: true
    prettierEslintOptions: {}
    prettierOptions:
      trailingComma: "all"
    silenceErrors: true
  tabs:
    enableVcsColoring: true
  "todo-show":
    findTheseTodos: [
      "XXX"
      "TODO"
      "FIXME"
      "CHANGED"
      "IDEA"
      "HACK"
      "NOTE"
      "REVIEW"
      "NB"
      "BUG"
      "QUESTION"
      "COMBAK"
      "TEMP"
    ]
    ignoreThesePaths: [
      "node_modules"
      "vendor"
      "bower_components"
      "flow-typed"
    ]
    sortBy: "Type"
  "tool-bar":
    position: "Left"
  "tree-view":
    alwaysOpenExisting: true
  "vim-mode-plus":
    blackholeRegisteredOperators: [
      "change*"
      "substitute*"
    ]
    notifiedCoffeeScriptNoLongerSupportedToExtendVMP: true
    useClipboardAsDefaultRegister: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
  zentabs:
    maximumOpenedTabs: 20
    neverCloseNew: true
".go.source":
  editor:
    showInvisibles: false
".reason.source":
  editor:
    preferredLineLength: 110
