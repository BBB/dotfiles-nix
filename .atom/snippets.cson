# ======= JavaScript

'.source.js':

  'console.log':
    'prefix': 'l'
    'body': 'console.log($1)'

  'const':
    'prefix': 'c'
    'body': 'const ${1:name} = ${0:value};'

  'import':
    'prefix': 'i'
    'body': 'import ${1:module} from \'${0:location}\';'

  'import type':
