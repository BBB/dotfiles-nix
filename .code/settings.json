{
  "editor.fontFamily": "Hermit, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 18,
  "editor.suggestFontSize": 14,
  "editor.fontWeight": "500",
  "editor.tabSize": 2,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": false,
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.scrollBeyondLastLine": false,
  "editor.renderLineHighlight": "all",
  "editor.roundedSelection": true,
  "editor.tabCompletion": true,
  "editor.suggestSelection": "recentlyUsed",
  "editor.snippetSuggestions": "inline",
  "editor.acceptSuggestionOnEnter": "on",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggestOnTriggerCharacters": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.rulers": [
    100
  ],
  "workbench.colorTheme": "Tomorrow Night",
  "workbench.iconTheme": "minimalistic-icons",
  "workbench.panel.defaultLocation": "bottom",
  "workbench.statusBar.visible": true,
  "workbench.startupEditor": "none",
  "workbench.editor.labelFormat": "medium",
  "workbench.editor.tabSizing": "shrink",
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorCustomizations": {
    "editor.background": "#292929",
    "editor.selectionBackground": "#ffffff40",
    "editor.selectionHighlightBackground": "#ffffff20",
    "editor.lineHighlightBackground": "#ffffff08",
    "editorIndentGuide.background": "#393939",
    "editorRuler.foreground": "#444",
    "editorHoverWidget.border": "#373737",
    "editorHoverWidget.background": "#373737",
    "editorInfo.foreground": "#3091ec",
    "editorWarning.foreground": "#ecd030",
    "editorSuggestWidget.background": "#444",
    "editorSuggestWidget.border": "#444",
    "editorSuggestWidget.selectedBackground": "#333",
    "sideBar.background": "#212121",
    "sideBar.border": "#212121",
    "gitDecoration.ignoredResourceForeground": "#676767",
    "gitDecoration.untrackedResourceForeground": "#b5bd68",
    "statusBar.background": "#232425",
    "statusBar.foreground": "#999",
    "statusBar.noFolderBackground": "#232425",
    "statusBar.debuggingBackground": "#511f1f",
    "scrollbar.shadow": "#00000000",
    "terminal.ansiBlack": "#383d43",
    "terminal.ansiRed": "#c66363",
    "terminal.ansiGreen": "#c0c86d",
    "terminal.ansiYellow": "#eac171",
    "terminal.ansiBlue": "#80a2be",
    "terminal.ansiMagenta": "#b193ba",
    "terminal.ansiCyan": "#90c9c1",
    "terminal.ansiWhite": "#c2c5c3",
    "terminal.ansiBrightBlack": "#636363",
    "terminal.ansiBrightRed": "#a04041",
    "terminal.ansiBrightGreen": "#8b9440",
    "terminal.ansiBrightYellow": "#ec9c62",
    "terminal.ansiBrightBlue": "#5d7f9a",
    "terminal.ansiBrightMagenta": "#82658c",
    "terminal.ansiBrightCyan": "#5e8d87",
    "terminal.ansiBrightWhite": "#6d757d"
  },
  "window.newWindowDimensions": "fullscreen",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "all",
  "window.zoomLevel": 0,
  "explorer.autoReveal": false,
  "explorer.openEditors.visible": 0,
  "search.useIgnoreFiles": true,
  "problems.decorations.enabled": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": "500",
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "zenMode.hideActivityBar": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false,
  "extensions.autoUpdate": false,
  "simpleIcons.hideArrows": false,
  "vim.useSystemClipboard": true,
  "reason.codelens.enabled": false,
  "reason.diagnostics.tools": [
    "bsb"
  ],
  "go.formatTool": "goimports",
  "javascript.validate.enable": false,
  "flow.enabled": false,
  "flow.useNPMPackagedFlow": true,
  "docker.showExplorer": false,
  "todohighlight.keywords": [
    {
      "text": "TODO:",
      "color": "#b294bb",
      "backgroundColor": "#292929",
    },
    {
      "text": "FIXME:",
      "color": "#b294bb",
      "backgroundColor": "#292929",
    },
    {
      "text": "HACK:",
      "color": "#b294bb",
      "backgroundColor": "#292929",
    },
    {
      "text": "XXX:",
      "color": "#b294bb",
      "backgroundColor": "#292929",
    }
  ],
  "todohighlight.include": [
    "**/*.re",
    "**/*.rei",
    "**/*.go",
    "**/*.js",
    "**/*.jsx",
    "**/*.css",
    "**/*.scss"
  ],
  "cSpell.showStatus": false,
  "cSpell.enabledLanguageIds": [
    "css",
    "go",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "plaintext",
    "reason",
    "restructuredtext",
    "scss",
    "text",
    "yml"
  ],
  "cSpell.userWords": [
    "signup",
    "todos",
    "graphql",
    "styleguide",
    "unmount",
    "validators",
    "stringify",
    "regexes",
    "nbsp"
  ],
  "cSpell.languageSettings": [
    {
      "languageId": "go",
      "allowCompoundWords": true,
      "dictionaries": [
        "go"
      ],
      "userWords": [
        "gonic"
      ]
    }
  ],
  "[reason]": {
    "editor.rulers": [
      80
    ]
  },
  "[markdown]": {
    "editor.formatOnSave": false,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false
  }
}
