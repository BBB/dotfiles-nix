# Roots
alias ~="cd ~"
alias dev="cd ~/Dev"
alias pro="cd ~/Dev/Projects"
alias sys="cd ~/Dev/System"
alias libs="cd ~/Dev/Libs"
alias sand="cd ~/Dev/Sandboxes"


# Projects
alias ___dotfiles="tmuxinator start dotfiles"

alias ___generator-flux-on-rails="tmuxinator start app generator-flux-on-rails root=~/Dev/Libs/generator-flux-on-rails"
alias ___generator-react-starter="tmuxinator start app generator-react-starter root=~/Dev/Libs/generator-react-starter"
alias ___fly-build-utils="tmuxinator start app fly-build-utils root=~/Dev/Libs/fly-build-utils"

alias ___bootstrap-loader="tmuxinator start app bootstrap-loader root=~/Dev/Projects/shakacode.com/oss/bootstrap-loader"
alias ___sass-resources-loader="tmuxinator start app sass-resources-loader root=~/Dev/Projects/shakacode.com/oss/sass-resources-loader"
alias ___react-webpack-rails-tutorial="tmuxinator start shaka-project react-webpack-rails-tutorial root=~/Dev/Projects/shakacode.com/oss/react-webpack-rails-tutorial"

alias ___fng="tmuxinator start shaka-project fng root=~/Dev/Projects/shakacode.com/fng"
